{
  "name": "tailwindcss-wordpress-starter",
  "version": "1.0.0",
  "description": "A WordPress starter theme built using Underscores, TailWind CSS and Webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "webpack --watch",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/_s.pot' --domain 'your_theme_name'"
  },
  "keywords": [
    "wordpress",
    "webpack",
    "tailwindcss"
  ],
  "author": "Kaleshe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrieforde/your_theme_namewith-webpack/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^3.6.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-prettier": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.3.0",
    "img-loader": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^7.0.35",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "~3.0.0",
    "postcss-sass-loader": "^1.1.0",
    "precss": "^4.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-wordpress": "^15.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "svg-sprite-loader": "^4.3.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "wp-pot-cli": "^1.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "aos": "^2.3.4",
    "resolve-url-loader": "^3.1.2"
  }
}
